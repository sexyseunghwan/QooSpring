package com.test.qoologin;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

@Service
public class TuningService implements ITuningService {
	
	@Autowired
	private TuningDAO dao;
	

	@Override
	public void s1(String qoouser_id, String qoouser_pw, String qoouser_birthday, String qoouser_email,
			String qoouser_gender, String qoouser_nation, String qoouser_ipaddress, int qoouser_hascoin,
			String qoouser_phone_num, int qoouser_grade, String qoouser_receive_email, String qoouser_receive_sms,
			String qoouser_denide, String qoouser_register_datetime, String qoouser_lastlogin_datetime,
			String qoouser_lastlogin_ipaddress) 
	{
		
		
		dao.userMaker( qoouser_id,  qoouser_pw,  qoouser_birthday,  qoouser_email,
				 qoouser_gender,  qoouser_nation,  qoouser_ipaddress,  qoouser_hascoin,
				 qoouser_phone_num,  qoouser_grade,  qoouser_receive_email,  qoouser_receive_sms,
				 qoouser_denide,  qoouser_register_datetime,  qoouser_lastlogin_datetime,
				 qoouser_lastlogin_ipaddress);
		
	}
	
}
